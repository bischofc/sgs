setEPS()
foo <- read.table("simulation.log", head=F)

postscript("test_total.pdf")
plot(foo$V1/60, foo$V2, type='l', xlab="Time (h)", ylab="Available Energy (Wh)");
postscript("test_produced.pdf")
plot(foo$V1, foo$V3, type='l', xlab="Time (h)", ylab="Produced Energy (Wh)");
postscript("test_consumed.pdf")
plot(foo$V1, foo$V4, type='l', xlab="Time (h)", ylab="Consumed Energy (Wh)");

